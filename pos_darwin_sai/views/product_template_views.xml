<?xml version="1.0" encoding="UTF-8"?>
<odoo>

<record id="product_template_view_inh_sai_form" model="ir.ui.view">
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view" />
    <field name="arch" type="xml">
        <xpath expr="//notebook" position="inside">
            <page string="Vademecum">
              <group colspan="4">
                <field name="active_principle_id"/>
                <label for="symptom_ids" string="Symptoms"></label>
                <field name="symptom_ids" nolabel="1"/>
              </group>
            </page>
        </xpath>
    </field>
</record>
 

</odoo>
