#. module: sale_approval
#: model:mail.template,body_html:sale_approval.email_template_sale_approval_mail
msgid "\n"
"               <p> Dear Sir / Madam,</p> \n"
"\n"
"% if not object.next_discount_amount:   \n"
"<p>Requesting your approval on the quotation ${object.name}.</p>\n"
"% endif\n"
"\n"
"% if object.next_discount_amount:               \n"
"<p>Kindly Requesting your approval for providing a discount of ${object.get_discount()}% on the quotation ${object.name}.</p>\n"
"% endif\n"
"\n"
"<p><b>Notes:</b></p>\n"
"<p>${object.get_reason_notes()}</p>  \n"
"\n"
"    <p>Thank you</p>   \n"
"            "
msgstr "\n"
"               <p> Estimado Señor / Señora,</p> \n"
"\n"
"% if not object.next_discount_amount:   \n"
"<p>Solicito su aprobación para la orden de venta ${object.name}.</p>\n"
"% endif\n"
"\n"
"% if object.next_discount_amount:               \n"
"<p>Respetuosamente solicito su aprobación para dar un descuento de ${object.get_discount()}% en la orden de venta ${object.name}.</p>\n"
"% endif\n"
"\n"
"<p><b>Notas:</b></p>\n"
"<p>${object.get_reason_notes()}</p>  \n"
"\n"
"    <p>Muchas gracias.</p>   \n"
"            "

